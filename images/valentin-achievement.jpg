<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="achieveBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f093fb"/>
      <stop offset="50%" style="stop-color:#f5576c"/>
      <stop offset="100%" style="stop-color:#4facfe"/>
    </linearGradient>
    <linearGradient id="goldGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f39c12"/>
      <stop offset="50%" style="stop-color:#f1c40f"/>
      <stop offset="100%" style="stop-color:#e67e22"/>
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#achieveBg)"/>
  
  <!-- Podium -->
  <g transform="translate(200,200)">
    <!-- First place (center) -->
    <rect x="-30" y="-40" width="60" height="80" fill="#f39c12"/>
    <text x="0" y="10" text-anchor="middle" font-family="serif" font-size="24" font-weight="bold" fill="white">1</text>
    
    <!-- Second place (left) -->
    <rect x="-100" y="-20" width="50" height="60" fill="#95a5a6"/>
    <text x="-75" y="20" text-anchor="middle" font-family="serif" font-size="20" font-weight="bold" fill="white">2</text>
    
    <!-- Third place (right) -->
    <rect x="50" y="-10" width="50" height="50" fill="#cd7f32"/>
    <text x="75" y="25" text-anchor="middle" font-family="serif" font-size="18" font-weight="bold" fill="white">3</text>
  </g>
  
  <!-- Winner figure on first place -->
  <g transform="translate(200,140)">
    <!-- Body -->
    <rect x="-12" y="15" width="24" height="45" rx="4" fill="#2c3e50"/>
    
    <!-- Head -->
    <circle cx="0" cy="0" r="18" fill="#f4d1a4"/>
    
    <!-- Crown -->
    <path d="M-15,-18 L-8,-25 L-3,-18 L3,-25 L8,-18 L15,-25 L18,-15 L-18,-15 Z" 
          fill="url(#goldGrad)" stroke="#e67e22" stroke-width="1"/>
    <circle cx="-8" cy="-22" r="2" fill="#e74c3c"/>
    <circle cx="0" cy="-25" r="3" fill="#e74c3c"/>
    <circle cx="8" cy="-22" r="2" fill="#e74c3c"/>
    
    <!-- Arms raised in victory -->
    <rect x="-30" y="20" width="8" height="20" rx="4" fill="#f4d1a4" transform="rotate(-30)"/>
    <rect x="22" y="20" width="8" height="20" rx="4" fill="#f4d1a4" transform="rotate(30)"/>
    
    <!-- Trophy in hands -->
    <g transform="translate(0,-10)">
      <rect x="-4" y="-20" width="8" height="15" rx="2" fill="url(#goldGrad)"/>
      <circle cx="0" cy="-25" r="6" fill="url(#goldGrad)"/>
      <rect x="-6" y="-23" width="12" height="3" fill="url(#goldGrad)"/>
      <rect x="-2" y="-5" width="4" height="8" fill="url(#goldGrad)"/>
    </g>
  </g>
  
  <!-- Flying awards and medals -->
  <g>
    <!-- Medal 1 -->
    <g transform="translate(80,80)">
      <circle cx="0" cy="0" r="12" fill="url(#goldGrad)" stroke="#e67e22" stroke-width="2"/>
      <path d="M-8,-15 L-3,-25 L3,-25 L8,-15" stroke="#e74c3c" stroke-width="3" fill="none"/>
      <text x="0" y="3" text-anchor="middle" font-family="serif" font-size="10" font-weight="bold" fill="white">★</text>
    </g>
    
    <!-- Medal 2 -->
    <g transform="translate(320,100)">
      <circle cx="0" cy="0" r="10" fill="url(#goldGrad)" stroke="#e67e22" stroke-width="2"/>
      <path d="M-6,-12 L-2,-20 L2,-20 L6,-12" stroke="#e74c3c" stroke-width="3" fill="none"/>
      <text x="0" y="3" text-anchor="middle" font-family="serif" font-size="8" font-weight="bold" fill="white">♕</text>
    </g>
    
    <!-- Trophy -->
    <g transform="translate(350,60)">
      <rect x="-3" y="5" width="6" height="15" rx="1" fill="url(#goldGrad)"/>
      <circle cx="0" cy="0" r="8" fill="url(#goldGrad)"/>
      <rect x="-10" y="-2" width="20" height="4" fill="url(#goldGrad)"/>
      <rect x="-1" y="20" width="2" height="6" fill="url(#goldGrad)"/>
    </g>
    
    <!-- Certificate -->
    <g transform="translate(50,180)">
      <rect x="-15" y="-10" width="30" height="20" rx="2" fill="white" stroke="#e67e22" stroke-width="2"/>
      <circle cx="10" cy="10" r="8" fill="url(#goldGrad)"/>
      <path d="M8,6 L12,10 L8,14" stroke="white" stroke-width="2" fill="none"/>
    </g>
  </g>
  
  <!-- Confetti and celebration -->
  <g fill="#f39c12" opacity="0.8">
    <rect x="100" y="50" width="3" height="8" transform="rotate(45)"/>
    <rect x="300" y="70" width="3" height="8" transform="rotate(-30)"/>
    <rect x="150" y="90" width="3" height="8" transform="rotate(60)"/>
    <rect x="250" y="110" width="3" height="8" transform="rotate(-45)"/>
  </g>
  
  <!-- Fireworks -->
  <g stroke="#f39c12" stroke-width="2" fill="none" opacity="0.7">
    <g transform="translate(120,40)">
      <line x1="0" y1="0" x2="8" y2="-8"/>
      <line x1="0" y1="0" x2="8" y2="8"/>
      <line x1="0" y1="0" x2="-8" y2="-8"/>
      <line x1="0" y1="0" x2="-8" y2="8"/>
    </g>
    
    <g transform="translate(280,50)">
      <line x1="0" y1="0" x2="6" y2="-6"/>
      <line x1="0" y1="0" x2="6" y2="6"/>
      <line x1="0" y1="0" x2="-6" y2="-6"/>
      <line x1="0" y1="0" x2="-6" y2="6"/>
    </g>
  </g>
  
  <!-- Title -->
  <text x="200" y="290" text-anchor="middle" font-family="serif" font-size="16" font-weight="bold" fill="white">
    Великие достижения
  </text>
</svg>