# Автономный AI Агент - Конфигурация API ключей
# Скопируйте этот файл в .env и заполните ваши API ключи

# OpenAI API (обязательно для большинства стратегий)
OPENAI_API_KEY=your_openai_api_key

# Фриланс платформы
UPWORK_CLIENT_ID=your_upwork_client_id
UPWORK_CLIENT_SECRET=your_upwork_client_secret
FIVERR_API_KEY=your_fiverr_api_key

# Криптовалютные биржи  
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key

# Социальные сети и боты
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret

# База данных (опционально)
DATABASE_URL=sqlite:///agent_data.db
REDIS_URL=redis://localhost:6379

# Настройки агента
DAILY_EARNING_GOAL=1.0
MAX_DAILY_TASKS=10
RISK_TOLERANCE=0.3
WORK_HOURS_START=9
WORK_HOURS_END=18